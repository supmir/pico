<div class="w-full">
  <h1 class="font-bold text-4xl">Links#index</h1>
  <div class="flex w-full">
  <% @links.each do |link|%>
    <div class="flex w-full">
      <div>
        <%= link.title %>
      </div>
      <div class="flex-1">
        <%= link.href %>
      </div>
      <div>
        <%= link.clicks %>
      </div>
    </div>
    <% end %>
  </div>
</div>
